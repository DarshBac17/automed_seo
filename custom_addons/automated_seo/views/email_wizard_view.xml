<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
<record id="view_seo_send_email_wizard_form" model="ir.ui.view">
    <field name="name">seo.send.email.wizard.form</field>
    <field name="model">seo.send.email.wizard</field>
    <field name="arch" type="xml">
        <form string="Send SEO Preview2">
            <sheet>
                <group>
                    <field name="feedback"/>

                </group>

            </sheet>
            <footer>
                <button string="Send" name="action_send_email" type="object" class="btn-primary"/>
                <button string="Cancel" class="btn-secondary" special="cancel"/>
            </footer>
        </form>
    </field>
</record>
</odoo>