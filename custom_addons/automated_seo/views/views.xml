<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record model="ir.ui.view" id="automated_seo.views_tree">
        <field name="name">Views</field>
        <field name="model">ir.ui.view</field>
        <field name="arch" type="xml">
            <tree string="Views">
                <field name="name"/>
                <field name="type"/>
                <field name="model"/>
                <field name="create_date"/>
                <field name="inherit_id"/>
            </tree>
        </field>
    </record>


    <record model="ir.ui.view" id="automated_seo.views_form">
        <field name="name">Views</field>
        <field name="model">ir.ui.view</field>
        <field name="arch" type="xml">
            <form string="PHP Mapper">
                <group>
                    <field name="name"/>
                    <field name="type"/>
                    <field name="model"/>
<!--                    <field name="create_date" widget="datetime"/>-->
                    <field name="inherit_id"/>
                </group>
            </form>
        </field>
    </record>


    <record model="ir.actions.act_window" id="automated_seo.views_action">
        <field name="name">Views</field>
        <field name="res_model">ir.ui.view</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
