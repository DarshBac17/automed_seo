<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="qweb_demo_snippets" inherit_id="website.snippets" name="Qweb snippets">
        <xpath expr="//div[@id='snippet_structure']" position="inside">
            <div class="o_panel_body">
                <t t-snippet="qweb_demo.demo_template"
                   t-thumbnail="/website/static/src/img/snippets_thumbs/s_banner.svg">
                    <keywords>dynamic, qweb, demo</keywords>
                </t>
            </div>
        </xpath>
    </template>
</odoo>